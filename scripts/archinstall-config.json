{
    "config_version": "2.8.1",
    "archinstall-language": "English",
    "bootloader": "grub-install",
    "debug": false,
    "disk_config": {
        "config_type": "pre_mounted_config",
        "mountpoint": "/mnt/archinstall"
    },
    "hostname": "archtemplate",
    "kernels": ["linux"],
    "locale_config": {
        "kb_layout": "us",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "network_config": {
        "type": "nm"
    },
    "ntp": true,
    "packages": [
        "wget",
        "curl",
        "sudo",
        "openssh",
        "base-devel",
        "git",
        "vim",
        "networkmanager",
        "grub",
        "efibootmgr"
    ],
    "parallel_downloads": 5,
    "profile_config": {
        "profile": null
    },
    "swap": false,
    "timezone": "America/Los_Angeles"
}
